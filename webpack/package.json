{
  "name": "@snowinmars/webpack",
  "version": "1.0.0",
  "main": ".",
  "types": ".",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=development BABEL_ENV=development webpack-dev-server --env development --hot --progress",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --env production --env sourceMap",
    "lint": "eslint --ext .ts src/ --color",
    "upd": "yarn upgrade @snowinmars/eslint-config",
    "analyze": "webpack --env production --env analyze",
    "srcmap": "npx source-map-explorer dist/**/*.js",
    "depcheck": "npx depcheck"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "convert-units": "^2.3.4"
  },
  "devDependencies": {
    "@snowinmars/eslint-config": "file:../eslint-config",
    "@types/case-sensitive-paths-webpack-plugin": "2.1.6",
    "@types/dotenv-webpack": "7.0.3",
    "@types/node": "18.6.2",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@types/webpack-env": "1.17.0",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "depcheck": "1.4.3",
    "dotenv-expand": "8.0.3",
    "dotenv-webpack": "8.0.0",
    "eslint": "8.20.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "serve": "14.0.1",
    "source-map-explorer": "2.5.2",
    "source-map-loader": "4.0.0",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  }
}
